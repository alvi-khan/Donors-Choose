<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="donorschoose.FXMLFiles.RegistrationPageController">
    <stylesheets>
        <URL value="@registrationpage.css" />
    </stylesheets>
   <children>
      <TextField fx:id="username" layoutX="198.0" layoutY="103.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="password" layoutX="198.0" layoutY="158.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Button fx:id="loginButton" layoutX="490.0" layoutY="356.0" mnemonicParsing="false" onAction="#loadLoginPage" styleClass="primaryButton" text="Login">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="458.0" layoutY="331.0" text="Already registered?">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="registerButton" layoutX="255.0" layoutY="230.0" mnemonicParsing="false" onAction="#register" prefHeight="39.0" prefWidth="89.0" styleClass="secondaryButton" text="Register">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
